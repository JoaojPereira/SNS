{
  "name": "Tema SNS Saude Manchester",
  "dataColors": [
    "#004B87", 
    "#FF0000", 
    "#FF8C00", 
    "#FFD700", 
    "#008000", 
    "#0000FF", 
    "#466D8C", 
    "#A9A9A9"
  ],
  "background": "#FFFFFF",
  "foreground": "#004B87",
  "tableAccent": "#004B87",
  "visualStyles": {
    "page": {
      "*": {
        "background": [
          {
            "color": { "solid": { "color": "#F3F4F6" } },
            "transparency": 0
          }
        ]
      }
    },
    "*": {
      "*": {
        "title": [
          {
            "show": true,
            "fontColor": { "solid": { "color": "#004B87" } },
            "fontSize": 14,
            "fontFamily": "Segoe UI",
            "bold": true,
            "alignment": "Left"
          }
        ],
        "background": [
          {
            "show": true,
            "color": { "solid": { "color": "#FFFFFF" } },
            "transparency": 0
          }
        ],
        "border": [
          {
            "show": true,
            "radius": 10,
            "color": { "solid": { "color": "#FFFFFF" } }
          }
        ],
        "dropShadow": [
          {
            "show": true,
            "color": { "solid": { "color": "#000000" } },
            "position": "Outer",
            "preset": "Custom",
            "shadowSpread": 2,
            "shadowBlur": 10,
            "angle": 90,
            "distance": 2,
            "transparency": 90
          }
        ],
        "visualTooltip": [
          {
            "titleFontColor": { "solid": { "color": "#FFFFFF" } },
            "valueFontColor": { "solid": { "color": "#FFFFFF" } },
            "background": { "solid": { "color": "#004B87" } }
          }
        ]
      }
    },
    "card": {
      "*": {
        "labels": [
          {
            "color": { "solid": { "color": "#666666" } },
            "fontSize": 12,
            "fontFamily": "Segoe UI"
          }
        ],
        "categoryLabels": [
          {
            "show": true,
            "color": { "solid": { "color": "#004B87" } },
            "fontSize": 20,
            "fontFamily": "DIN"
          }
        ]
      }
    }
  }
}